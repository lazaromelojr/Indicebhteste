     <div id="marcatopo">
        <a href="index.html"> <img src="/images/indice_vinho.jpg" alt="Indece BH" border="0" /></a>
     </div>
  </div>
  
  <div id="faixacontato"></div>
 
  <div id="menutopo">
  <ul>       
        <li> <a href="./indice_bhs"> <div class="btnindice"> </div> </a></li>
        <li> <a href="./servicosindices"> <div class="btnnossoservicos"> </div> </a></li>
        <li> <a href="./indiceconexaos"> <div class="btnindicetv"> </div> </a> </li>
        <li> <a href="./clientes"> <div class="btnclientes"> </div> </a> </li>
